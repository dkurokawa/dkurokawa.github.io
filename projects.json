    {
      "id": "check-nc-licenses",
      "name": "check-nc-licenses",
      "type": "npm",
      "github": "dkurokawa/check-nc-licenses",
      "description": "npm依存関係に含まれる非商用（NC）ライセンスを検出",
      "environment": "npm package",
      "install": {
        "npm": "npm install -g check-nc-licenses"
      }
    },
    {
